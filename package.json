{
  "name": "simple-music-player",
  "version": "1.0.0",
  "description": "A simple music player for mp3 files",
  "main": "main.js",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "build": {
    "appId": "simplemusicplayer",
    "mac": {
      "category": "Music"
    }
  },
  "author": "Nowilltolife",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^18.0.2",
    "electron-builder": "^22.14.13"
  },
  "dependencies": {
    "@electron/remote": "^2.0.8",
    "music-metadata": "^7.12.2"
  }
}
